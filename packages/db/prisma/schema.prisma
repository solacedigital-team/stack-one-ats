datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider     = "prisma-client"
  output       = "../src/generated/client"
  moduleFormat = "cjs"
  runtime      = "node"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}
